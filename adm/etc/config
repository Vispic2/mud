# 发布版本的服务器地址(local表示本地)
release server : local

# 版本服务器接收的客户端地址
& 127.0.0.1 : valid

# 本地能够接纳的最大用户数目
max users : 500

# 本地限制巫师登录的等级
& wiz lock : 0

# INTERNET互连网络中MUD的名字
internet mud name : 一个江湖

# INTERNET互连网络中MUD地址
my_ip : 113.142.140.78

# MUDOS
mudos : FluffOS v2019

# 编码方式
encoding : UTF-8

# SMTP服务器地址/端口号
& smtp server : smtp.qq.com 465

# SMTP服务器的名字
& smtp domain : smtp

# 系统使用SMTP服务器时填写的发送者
& smtp sender : 21315491@qq.com

# SMTP认证信息
smtp auth user : 21315491
smtp auth password : yilhafvabeigcgjg

# 和本系统相联系的MUD服务器
mudlist1 : 113.142.140.78 5002     #一个江湖

# 游戏服务区域
zone : NAME_MUD

# 玩家数据错误时是否要求重新创建玩家
& ask_recreate : 0
